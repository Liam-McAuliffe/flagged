{
  "hosting": {
    "site": "flagged",
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/game",
        "destination": "/game.html"
      },
      {
        "source": "/practice",
        "destination": "/practice.html"
      },
      {
        "source": "/login",
        "destination": "/login.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
